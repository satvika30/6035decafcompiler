class Program {
  int x, z, A[10];
  boolean c;
  
  void foo() {}

  void main() {
  	int a, b;
  	
  	// Load z here
  	
  	for i = 0, 10 {
 		x = z + 7;
 		
  		foo();
  		
  		// Load c, z, x, A[z] here
  		
  		for j = 0, 10 {
  			if (c) {
  				x = 90;
  			}
  			b = A[z];
  			
  			// Load A[x]
  			
  			for k = 0, 10 {
  				a = A[x];
  			}
  		}
  	}
  	
  	z = a + b;
  }
}