class Program {
  int x, z, A[10];
  
  void foo() {}

  void main() {
  	int a, b;
  	
  	// Load z here
  	
  	for i = 0, 10 {  		
 		x = z + 7;
 		
  		foo();
  		
  		// Load z, x, A[z], A[x] here
  		
  		for j = 0, 10 {
  			b = A[z];
  			for k = 0, 10 {
  				a = A[x];
  			}
  		}
  	}
  	
  	z = a + b;
  }
}