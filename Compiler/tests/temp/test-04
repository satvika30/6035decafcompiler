		a = y;
	}
	x = y + x;
	b = a + y + z;
	z = 9;
	A[x] = 9;
  }
  
  int bar() {
  	int a, b;
  	b = -1000;
  	a = -b;
  	return a;
  }

  void main() {
  	//x = A[A[A[1]]];
