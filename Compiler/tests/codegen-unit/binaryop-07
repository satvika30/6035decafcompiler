class Program
{
    void main()
    {
   		boolean a, b, c; 
   		
   		a = true;
   		
   		b = false;

		c = (false || (false || ( false || false || false || false || (false || true))));
		
		callout("%d\n", c);  //should give 1
	
		c = ((false && false) || (true && (false || true))) && ( 30 < 100);
		
		callout("printf", "%d\n", c);  //should give 1
		
	    c = !((false && false) || (true && (false || true))) && ( !!(30 < 100));
	    
		callout("printf", "%d\n", c);  //should give 0
	
    }
}
