class Program {
	void main() {
		for i = 0, 10 {
			if (i > 2) {
				break;
			}
			
			for j = 0, 10 {
				if (j > 3) {
					break;
				}

				callout("printf","%d\n",j);
			}
		}
		
		callout("printf","-\n");
		
		for i = 0, 10 {
			if (i < 7) {
				continue;
			}
			
			for j = 0, 10 {
				if (j < 8) {
					continue;
				}
				
				callout("printf","%d\n",j);
			}
		}
	}
}