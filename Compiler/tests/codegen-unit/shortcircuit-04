class Program
{
	int A;
   	boolean changeA()
    {
        callout("printf","executing second cond\n");
        return true;
    }


    void main()
    {
		A=0;

        if (false && changeA())
	{
        callout("printf","A = %d\n", A);
	} else {
		callout("printf","A = %d\n", A);
	}
     
    }

}

