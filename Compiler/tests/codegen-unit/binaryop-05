class Program
{
    void main()
    {
   		boolean a, b, c, d, e, f; 
   		a = false;
   		b = true;
   		
   		c = a || !b;
   		
   		callout("printf", "%d\n", c); //should give 0
   		
   		c = !a && b;
   		
   		callout("printf", "%d\n", c);  //should give 1
   		
   		c = (true && false) || (false || true);
   		
		callout("printf", "%d\n", c);  //should give 1
   		
   		c = !!(true);
   		
		callout("printf", "%d\n", c);  //should give 1
 
    }
}
