class Program {
   boolean greater_than_20_and_true(boolean a, int b, int c, int d, int e, int f, int g, boolean h) {
   		if (a) {
   		  if ((b+c+d+e+f+g) > 20) {
   		  	if (h) {
   		  	  return true;
   		  	} else {
   		  	  return false; 
   		  	}  		  
   		  } else {
   		  	return false;
   		  }
   		} else {
   		  return false;
   		}
   }

   void main() {
      boolean a, c;
      int ret;
      a = true;
      c = true;
      if (greater_than_20_and_true(a,1,2,3,4,5,6,c)) {
        ret = 1;
      } else {
        ret = 0;
      }
      callout("printf","%d",ret); // 1
   }
}
