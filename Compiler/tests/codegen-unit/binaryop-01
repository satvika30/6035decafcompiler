class Program
{
    void main()
    {
   		int a, b, c;
   		callout("printf", "a: %d\n", a);
   		callout("printf", "b: %d\n", b);
   		callout("printf", "c: %d\n", c);

        a = 10;
        b = 20;

        c = a + b;

        callout("printf", "c = a + b, c: %d\n", c);
        
        c = a - b;
        
        callout("printf", "c = a - b, c: %d\n", c);
        
        c = a - 10;

        callout("printf", "c = a - 10, c: %d\n", c);
    }
}
