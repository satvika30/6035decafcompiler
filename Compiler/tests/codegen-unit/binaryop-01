class Program {
    void main() {
   		int a, b, c, d, e ,f;
   		
        a = 2 * 4;
        callout("printf", "%d\n", a);
        
        a = a * 2 / 4;
        callout("printf", "%d\n", a);
        
        b = 7;        
        c = a * (b / 2);
        callout("printf", "%d\n", c);
        
        f = 100 / 11 * b; 
        callout("printf", "%d\n", f);
        
        e = 100 / -11 * b;
        callout("printf", "%d\n", e);
        
        e = 100 / -11 * -b;
        callout("printf", "%d\n", e);
        
        a = 100 % 7 * 2;
        callout("printf", "%d\n", a);
    }
}
